{
  "name": "ai-atl",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "backend",
      "frontend",
      "demo-user"
    ]
  },
  "scripts": {
    "dev:backend": "pnpm --filter @fledgling/tracer dev",
    "dev:frontend": "pnpm --filter ai-atl-frontend dev",
    "dev:demo": "pnpm --filter @ai-atl/demo-user dev",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\" \"pnpm run dev:demo\"",
    "build": "pnpm --filter @fledgling/tracer build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
